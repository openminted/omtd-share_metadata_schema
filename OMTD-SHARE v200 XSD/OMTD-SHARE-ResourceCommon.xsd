<xs:schema xmlns:ms="http://www.meta-share.org/OMTD-SHARE_XMLSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.meta-share.org/OMTD-SHARE_XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.0.1" xml:lang="en">
	<xs:include schemaLocation="./OMTD-SHARE-GlobalTypes.xsd"/>
	<xs:include schemaLocation="./OMTD-SHARE-ISOVocabularies.xsd"/>
	<xs:include schemaLocation="./OMTD-SHARE-Actors.xsd"/>
	<xs:include schemaLocation="./OMTD-SHARE-Projects.xsd"/>
	<xs:include schemaLocation="./OMTD-SHARE-Distributions.xsd"/>
	<!-- definition of complex types -->
	<xs:complexType name="mailingListInfoType">
		<xs:annotation>
			<xs:appinfo>
				<render-short>{mailingListName}</render-short>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="mailingListName" type="xs:string">
				<xs:annotation>
					<xs:documentation>The name of the mailing list</xs:documentation>
					<xs:appinfo>
						<label>Mailing list name</label>
						<label-plural>Mailing list names</label-plural>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="subscribe" type="ms:emailAddress" minOccurs="1">
				<xs:annotation>
					<xs:documentation>The email address to be used for subscribing to the mailing list</xs:documentation>
					<xs:appinfo>
						<label>Subscribe</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="unsubscribe" type="ms:emailAddress" minOccurs="1">
				<xs:annotation>
					<xs:documentation>The email address to be used for unsubscribing from the mailing list</xs:documentation>
					<xs:appinfo>
						<label>Unsubscribe</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="post" type="ms:emailAddress">
				<xs:annotation>
					<xs:documentation>The email address to be used for posting messages to the mailing list</xs:documentation>
					<xs:appinfo>
						<label>Post</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="archive" type="ms:httpURI">
				<xs:annotation>
					<xs:documentation>The URL where the mailing list messages are archived</xs:documentation>
					<xs:appinfo>
						<label>Archive</label>
						<label-plural>Archives</label-plural>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="otherArchives" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="otherArchive" type="ms:httpURI" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Alternative ULRs where the mailing list messages are archived (for mirrored mailing lists)</xs:documentation>
								<xs:appinfo>
									<label>Other archive</label>
									<label-plural>Other archives</label-plural>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="identificationInfoType">
		<xs:sequence>
			<xs:element name="resourceNames">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="ms:resourceName" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="descriptions" minOccurs="1" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="description" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>A short free-text account that provides information about the resource (e.g. function, contents, technical information etc.)</xs:documentation>
								<xs:appinfo>
									<label>Description</label>
									<label-plural>Descriptions</label-plural>
									<usage-ugc>A corpus generated automatically by [user name] on [creation date] via OpenMinTeD services. The corpus includes [number of] publications from [Source] (in ([language]) and [domain]).</usage-ugc>
									<change>true</change>
									<exactMatch>dct:description</exactMatch>
									<closeMatch>http://hdl.handle.net/11459/CCR_C-2520_9eeedfb4-47d3-ddee-cfcb-99ac634bf1db (ccr:descriptionContext)</closeMatch>
									<closeMatch>datacite:description descriptionType="Abstract"</closeMatch>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:restriction base="ms:myString">
										<xs:maxLength value="10000"/>
									</xs:restriction>
								</xs:simpleContent>
							</xs:complexType>
							<xs:unique name="uniqueLang3">
								<xs:selector xpath="."/>
								<xs:field xpath="@lang"/>
							</xs:unique>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="resourceShortNames" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="resourceShortName" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>A short form (e.g. abbreviation, acronym etc.) used to identify the resource.</xs:documentation>
								<xs:appinfo>
									<label>Resource short name</label>
									<label-plural>Resource short names</label-plural>
									<recommended>true</recommended>
									<closeMatch>dct:alternative</closeMatch>
									<exactMatch>http://hdl.handle.net/11459/CCR_C-2544_3626545e-a21d-058c-ebfd-241c0464e7e5 (ccr:resourceName)</exactMatch>
									<!-- in DataCite, the title has the attribute "titleType" with values "alternativeTitle", "translatedTitle" and "subtitle"; we can do something similar in the next version if desired -->
									<closeMatch>datacite:title titleType="alternativeTitle"</closeMatch>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:restriction base="ms:myString">
										<xs:maxLength value="500"/>
									</xs:restriction>
								</xs:simpleContent>
							</xs:complexType>
							<xs:unique name="uniqueLang">
								<xs:selector xpath="."/>
								<xs:field xpath="@lang"/>
							</xs:unique>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="resourceIdentifiers" minOccurs="1" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="resourceIdentifier" type="ms:resourceIdentifierType" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Associates a resource to an identifier (e.g. PID, DOI, internal to an organization etc.), i.e. a string used to uniquely identify it</xs:documentation>
								<xs:appinfo>
									<label>Identifier</label>
									<label-plural>Identifiers</label-plural>
									<usage-components>prefill with Maven coordinates if known</usage-components>
									<usage-ugc>To be created automatically by the system</usage-ugc>
									<broadMatch>dct:identifier</broadMatch>
									<!-- we allow for all possible identifiers and not only persistent ones as in CCR -->
									<closeMatch>http://hdl.handle.net/11459/CCR_C-2573_ae7c2548-8a86-ab6e-7099-e28b7697d1a2 (ccr:persistentIdentifier)</closeMatch>
									<!-- currently DataCite only supports DOI's -->
									<exactMatch>dataCite:identifier</exactMatch>
								</xs:appinfo>
							</xs:annotation>
							<!-- CHECK: I have not been able to add the restriction of length 100 -->
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="public" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>Declares whether the resource is meant for private use or can be publicly available through the registry</xs:documentation>
					<xs:appinfo>
						<label>public</label>
						<change>true</change>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="contactInfoType">
		<xs:sequence>
			<xs:choice>
				<xs:element name="contactEmail" type="ms:emailAddress">
					<xs:annotation>
						<xs:documentation>A general email address that can be used as contact point for a resource (e.g. resource@example.com)</xs:documentation>
						<xs:appinfo>
							<label>Contact email</label>
							<label-plural>Contact emails</label-plural>
							<!-- in DataCite, the contactPerson can also be used for contact points -->
							<closeMatch>datacite:contributor contributorType="ContactPerson"</closeMatch>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="landingPage" type="ms:httpURI">
					<xs:annotation>
						<xs:documentation>A URL used as the landing page of a resource providing general information; for instance, it may present a description of the resource, its creators and possibly include links to the URL where it can be accessed from</xs:documentation>
						<xs:appinfo>
							<label>Landing page</label>
							<label-plural>Landing pages</label-plural>
							<exactMatch>dcat:landingPage</exactMatch>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="contactPersons" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="contactPerson" type="ms:personInfoType" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Groups information on the person(s) that is/are responsible for providing further information regarding the resource</xs:documentation>
									<xs:appinfo>
										<label>Contact person</label>
										<label-plural>Contact persons</label-plural>
										<usage>email must be mandatory for contact persons</usage>
										<exactMatch>dcat:contactPoint</exactMatch>
										<closeMatch>http://hdl.handle.net/11459/CCR_C-2454_e0e88868-2fc6-a87e-cd0d-d8f69c85b41d (ccr:contactFullName)</closeMatch>
										<relation>many-to-many</relation>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="contactGroups" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="contactGroup" type="ms:groupInfoType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Groups information on the group(s) that is/are responsible for providing further information regarding the resource</xs:documentation>
								<xs:appinfo>
									<label>Contact group</label>
									<label-plural>Contact groups</label-plural>
									<recommended>true</recommended>
									<exactMatch>dcat:contactPoint</exactMatch>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<!-- ASK googlegroups?? -->
			<xs:element name="mailingLists" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="mailingListInfo" type="ms:mailingListInfoType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Groups information on the mailing list(s) that can be used for further information regarding the resource</xs:documentation>
								<xs:appinfo>
									<label>Mailing list</label>
									<label-plural>Mailing list</label-plural>
									<usage-components>show in editor only for component providers</usage-components>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="versionInfoType">
		<xs:annotation>
			<xs:appinfo>
				<render-short>{version} {lastDateUpdated}</render-short>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="version">
					<xs:annotation>
						<xs:documentation>Associates a resource with a numeric pattern of the form major_version.minor_version.patch (in accordance to the semantic versioning guidelines, cf. http://semver.org) that identifies its version</xs:documentation>
						<xs:appinfo>
							<label>Version</label>
							<label-plural>Versions</label-plural>
							<default>1.0.0</default>
							<change>true</change>
							<exactMatch>dataCite:version</exactMatch>
							<closeMatch>http://hdl.handle.net/11459/CCR_C-2547_7883d382-b3ce-8ab4-7052-0138525a8ba1 (ccr:version)</closeMatch>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="100"/>
							<xs:pattern value="[0-9]{1,2}(\.[0-9]{1,2}(\.[0-9]{1,3}))"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="versionDate">
					<xs:annotation>
						<xs:documentation>Associates a resource with the last modification date (or creation date if it hasn't been modified); the preferred format is YYYYMMDD</xs:documentation>
						<xs:appinfo>
							<label>Version</label>
							<label-plural>Versions</label-plural>
							<exactMatch>dataCite:version</exactMatch>
							<closeMatch>http://hdl.handle.net/11459/CCR_C-2547_7883d382-b3ce-8ab4-7052-0138525a8ba1 (ccr:version)</closeMatch>
						</xs:appinfo>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="100"/>
							<xs:pattern value="[0-9]{8}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:choice>
			<xs:element name="versionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A type of versioning vocabulary used mainly for works (e.g. scholarly articles, books etc.) that go through a formal review process before final publication, marked with major changes across the versions</xs:documentation>
					<xs:appinfo>
						<label>Version type</label>
						<label-plural>Version types</label-plural>
						<usage-publications>show only for publications</usage-publications>
						<commentForSource>taken from OpenAIRE but added the definitions</commentForSource>
						<note>Some of these version types can be considered preprints in the sense denoted by COAR (http://purl.org/coar/resource_type/c_816b)</note>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="accepted">
							<xs:annotation>
								<xs:documentation>For versions that have been accepted for publication but are not yet published</xs:documentation>
								<xs:appinfo>
									<label>Accepted</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="published">
							<xs:annotation>
								<xs:documentation>For versions as they have been formally published in print or digital form</xs:documentation>
								<xs:appinfo>
									<label>Published</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="draft">
							<xs:annotation>
								<xs:documentation>For versions during the writing up process</xs:documentation>
								<xs:appinfo>
									<label>Draft</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="submitted">
							<xs:annotation>
								<xs:documentation>For versions as submitted for publication</xs:documentation>
								<xs:appinfo>
									<label>Submitted</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="updated">
							<xs:annotation>
								<xs:documentation>For versions that have been updated vis-a-vis the previous published version</xs:documentation>
								<xs:appinfo>
									<label>Updated</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="revision" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A short free-text account of the revisions made to the current version vis-a-vis the previous version or a link to a document reporting these revisions</xs:documentation>
					<xs:appinfo>
						<label>Revision</label>
						<label-plural>Revisions</label-plural>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="500"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="lastDateUpdated" type="ms:dateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date when the version of the resource being described was last updated</xs:documentation>
					<xs:appinfo>
						<label>Last date updated</label>
						<exactMatch>dct:modified</exactMatch>
						<exactMatch>http://hdl.handle.net/11459/CCR_C-2526_979ac535-eaa5-5e59-3cad-51c450234698 (ccr:lastUpdate)</exactMatch>
						<exactMatch>dataCite:date dateType="updated"</exactMatch>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="updateFrequency" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the frequency with which a new updated version of the resource is released (e.g. every 6 months or two years)</xs:documentation>
					<xs:appinfo>
						<label>Update frequency</label>
						<closeMatch>dct:frequency</closeMatch>
						<closeMatch>dcat:accrualPeriodicity</closeMatch>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="validationInfoType">
		<xs:sequence>
			<xs:element name="validated" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Specifies the validation status of the resource</xs:documentation>
					<xs:appinfo>
						<label>Validated</label>
						<exactMatch>http://hdl.handle.net/11459/CCR_C-2584_36b81c9c-e23a-2ace-36cd-b0d6679e3197 (ccr:validation)</exactMatch>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="validationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the type of the validation that have been performed </xs:documentation>
					<xs:appinfo>
						<label>Validation type</label>
						<exactMatch>http://hdl.handle.net/11459/CCR_C-2587_026dcaa6-8ece-3364-8492-6479e70f66de (ccr:validationType)</exactMatch>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
						<xs:enumeration value="formal">
							<xs:annotation>
								<xs:appinfo>
									<label>Formal</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="content">
							<xs:annotation>
								<xs:appinfo>
									<label>Content</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="validationMode" type="ms:processMode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the validation methodology applied</xs:documentation>
					<xs:appinfo>
						<label>Validation mode</label>
						<exactMatch>http://hdl.handle.net/11459/CCR_C-2586_75098d25-a517-983f-817c-2b05c5ce361a (ccr:validationMode)</exactMatch>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="validationModeDetails" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Textual field for additional information on validation</xs:documentation>
					<xs:appinfo>
						<label>Validation mode details</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="500"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="validationExtent" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The resource coverage in terms of validated data</xs:documentation>
					<xs:appinfo>
						<label>Validation extent</label>
						<exactMatch>http://hdl.handle.net/11459/CCR_C-2585_0fcf8ea5-a273-d077-86e9-5e8f1c50fe72 (ccr:validationLevel)</exactMatch>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
						<xs:enumeration value="full"/>
						<xs:enumeration value="partial"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="validationExtentDetails" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provides information on size or other details of partially validated data; to be used if only part of the resource has been validated and as an alternative to SizeInfo if the validated part cannot be counted otherwise</xs:documentation>
					<xs:appinfo>
						<label>Validation extent details</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="500"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="sizePerValidation" type="ms:sizeInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the size of the validated part of a resource</xs:documentation>
					<xs:appinfo>
						<label>Size per validation</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="validationReports" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="hasValidationReport" type="ms:relatedDocumentInfoType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Link to a short account (prose description) of the validation details or the identifier of a document with detailed information on the validation process and results</xs:documentation>
								<xs:appinfo>
									<label>Has validation report</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="validationSwComponents" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="isValidatedBy" type="ms:relatedResourceType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Links a resource to the identifier of the software component that has been used for its validation</xs:documentation>
								<xs:appinfo>
									<label>Is validated by</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="validators" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="validator" type="ms:actorInfoType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
