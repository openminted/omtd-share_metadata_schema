		<xs:annotation>
			<xs:documentation>Specifies the size of the resource with regard to the SizeUnit measurement in form of a number</xs:documentation>
			<xs:appinfo>
				<label>Size</label>
			</xs:appinfo>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="100"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="register">
		<xs:annotation>
			<xs:documentation>For corpora that have already been using register classification</xs:documentation>
			<xs:appinfo>
				<label>Register</label>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="classificationSchemeName" type="ms:classificationSchemeName" use="required"/>
					<xs:attribute name="schemeURI" type="xs:anyURI" use="optional"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="keyword" type="xs:string">
		<xs:annotation>
			<xs:documentation>A word or phrase considered important for the classification of a resource and included as tag in its metadata</xs:documentation>
