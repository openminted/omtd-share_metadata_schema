						<xs:enumeration value="ACA">
							<xs:annotation>
								<xs:documentation>For licences that allow the use of resources only by academic users (individuals affiliated with universities and/or research centers who are authenticated through a certified authentication system); other conditions of use may also apply</xs:documentation>
							<xs:appinfo>
								<label>ACA</label>
							</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="PUB">
							<xs:annotation>
								<xs:documentation>For licences that allow public use (i.e. resources can be accessed by all users, although other conditions of use may apply)</xs:documentation>
							<xs:appinfo>
								<label>PUB</label>
							</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="RES">
							<xs:annotation>
								<xs:documentation>For licences that allow restricted use of resources by users that are both authenticated and authorized; other conditions of use may also apply</xs:documentation>
							<xs:appinfo>
								<label>RES</label>
							</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
-->
		</xs:sequence>
	</xs:complexType>
	<!--
	<xs:complexType name="rightsStatementInfoType">
		<xs:annotation>
			<xs:documentation>Groups information on formal statements indicative of licensing terms for the use of a resource (e.g. open access, free to read, all rights reserved etc.); the semantics of the statements should be clear, preferrably formally expressed and stored at a URL</xs:documentation>
			<xs:appinfo>
				<label>Rights statement</label>
				<label>Rights statements</label>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
-->
	<xs:element name="rightsStatement">
		<xs:annotation>
			<xs:documentation>The name (identifier) of a formal statement indicative of licensing terms for the use of a resource (e.g. open access, free to read etc.); its semantics should be clear, preferrably formally expressed and stored at a URL</xs:documentation>
			<xs:appinfo>
				<label>Rights statement</label>
				<label-plural>Rights statements</label-plural>
			</xs:appinfo>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="openAccess">
					<xs:annotation>
						<xs:documentation>The free and online availability of literature, which allows to read, download, copy, distribute, print, search, or link to the ful text, crawl articles for indexing, pass them as data to software, or use them for any other useful purpose. An availability that is granted without financial, legal, or technical barriers other than those inseparable from gaining access to the internet itself, and those related to giving authors control over the integrity of their work and the right to be properly acknowledged and cited.</xs:documentation>
						<xs:appinfo>
							<label>Open Access</label>
							<closeMatch>info:eu-repo/semantics/openAccess</closeMatch>
							<closeMatch>http://purl.org/eprint/accessRights/OpenAccess</closeMatch>
						</xs:appinfo>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="restrictedAccess">
					<xs:annotation>
						<xs:documentation>Access is subject to restrictions or controls regarding users (e.g. limited to researchers or members of a specific group) or use (e.g. for research only, noncommercial use etc.); subscription or registration may also be required and access may incur financial costs.</xs:documentation>
						<xs:appinfo>
							<label>Restricted Access</label>
							<closeMatch>info:eu-repo/semantics/restrictedAccess</closeMatch>
							<closeMatch>http://purl.org/eprint/accessRights/RestrictedAccess</closeMatch>
						</xs:appinfo>
					</xs:annotation>
				</xs:enumeration>
				<!--
				<xs:enumeration value="embargoedAccess">
					<xs:annotation>
						<xs:documentation>The resource is with Closed Access, until released for Open Access on a certain date, which can be specified elsewhere in the metadata.</xs:documentation>
						<xs:appinfo>
							<label>Embargoed Access</label>
							<closeMatch>info:eu-repo/semantics/embargoedAccess</closeMatch>
						</xs:appinfo>
					</xs:annotation>
				</xs:enumeration>
-->
				<!-- not sufficiently different from restrictedAccess
				<xs:enumeration value="closedAccess">
					<xs:annotation>
						<xs:documentation>Opposite to open access; access limited to a specific number of people and/or corporate bodies, frequently after an official procedure of request and consent is followed.</xs:documentation>
						<xs:appinfo>
							<label>Closed Access</label>
							<closeMatch>info:eu-repo/semantics/closedAccess</closeMatch>
							<closeMatch>http://purl.org/eprint/accessRights/ClosedAccess</closeMatch>
						</xs:appinfo>
					</xs:annotation>
				</xs:enumeration>
-->
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<!--
			<xs:element name="rightsStmtURL" type="xs:string">
				<xs:annotation>
					<xs:documentation>Link to the URL with the text that formally explains the licensing conditions imposed by the statement</xs:documentation>
					<xs:appinfo>
						<label>Rights statement URL</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
-->
	<!-- definition of elements -->
	<xs:element name="documentDistributionInfo" type="ms:documentDistributionInfoType">
		<xs:annotation>
			<xs:documentation>Groups information on the distribution of a document (single file)</xs:documentation>
			<xs:appinfo>
				<relation>one-to-many</relation>
				<label>Document distribution</label>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="datasetDistributionInfo" type="ms:datasetDistributionInfoType">
		<xs:annotation>
			<xs:documentation>Groups information on the distribution of a dataset resource (multiple files)</xs:documentation>
			<xs:appinfo>
				<relation>one-to-many</relation>
				<label>Document distribution</label>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="componentDistributionInfo" type="ms:componentDistributionInfoType">
		<xs:annotation>
			<xs:documentation>Groups information on the distributable form of a component (e.g. as a software code, an executable programme, a web service etc.)</xs:documentation>
			<xs:appinfo>
				<relation>one-to-many</relation>
				<label>Document distribution</label>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="licenceInfo" type="ms:licenceInfoType">
		<xs:annotation>
			<xs:documentation>Groups information on licences for the resource; can be repeated to allow for different modes of access and restrictions of use (e.g. free for academic use, on-a-fee basis for commercial use, download of a sample for free use etc.)</xs:documentation>
			<xs:appinfo>
				<label>Licence</label>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<!--
	<xs:element name="rightsStatementInfo" type="ms:rightsStatementInfoType">
		<xs:annotation>
			<xs:documentation>Groups information on official statements indicative of licensing terms for the use of a resource (e.g. open access, free to read, all rights reserved etc.); its semantics should be clear, preferrably formally expressed and stored at a url</xs:documentation>
			<xs:appinfo>
				<label>Rights statement</label>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
-->
	<xs:element name="rightsInfo">
		<xs:annotation>
			<xs:documentation>Groups information on licensing terms for the distribution of a resource; at least a licence (preferred option) or formal rights statement (e.g. open access) must be declared for the resource</xs:documentation>
			<xs:appinfo>
				<label>Information on distribution rights</label>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:element name="licenceInfos">
						<xs:complexType>
							<xs:sequence>
								<xs:element ref="ms:licenceInfo" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Groups information on licences for the resource; can be repeated to allow for different modes of access and restrictions of use (e.g. free for academic use, on-a-fee basis for commercial use, download of a sample for free use etc.)</xs:documentation>
										<xs:appinfo>
											<relation>many-to-many</relation>
											<label>Licence</label>
										</xs:appinfo>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<!--
					<xs:element ref="ms:rightsStatementInfo" minOccurs="0"/>
-->
					<xs:element ref="ms:rightsStatement" minOccurs="0"/>
				</xs:sequence>
				<xs:element ref="ms:rightsStatement" minOccurs="1"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- definition of elements -->
	<xs:element name="distributionMedium">
		<xs:annotation>
			<xs:documentation>Specifies the medium (channel) used for delivery or providing access to the resource</xs:documentation>
			<xs:appinfo>
				<recommended>true</recommended>
				<relation>one-to-many</relation>
				<label>Distribution medium</label>
			</xs:appinfo>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="30"/>
				<xs:enumeration value="webExecutable">
					<xs:annotation>
						<xs:documentation>For web services</xs:documentation>
						<xs:appinfo>
							<label>Web executable</label>
						</xs:appinfo>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="downloadable">
					<xs:annotation>
						<xs:documentation>For resources that can be downloaded, e.g. for documents that can be downloaded or software tools that can be locally downloaded and run</xs:documentation>
						<xs:appinfo>
							<label>Downloadable</label>
						</xs:appinfo>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="accessibleThroughInterface">
					<xs:annotation>
						<xs:documentation>For resources that can accessed only via an interface, e.g. lexica or corpora through query interfaces</xs:documentation>
						<xs:appinfo>
							<label>Acessible through interface</label>
						</xs:appinfo>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="other">
					<xs:annotation>
						<xs:documentation>Value for distributionMedium; to be used when the desired value is not included in the listed values</xs:documentation>
						<xs:appinfo>
							<label>Other</label>
						</xs:appinfo>
					</xs:annotation>
				</xs:enumeration>
				<!--
				<xs:enumeration value="paperCopy">
		<xs:annotation>
			<xs:documentation>For resources that are available only in paper format</xs:documentation>
			<xs:appinfo>
				<label>Paper copy</label>
			</xs:appinfo>
		</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="hardDisk"/>
				<xs:enumeration value="bluRay"/>
				<xs:enumeration value="DVD-R"/>
				<xs:enumeration value="CD-ROM"/>
-->
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="distributionURL" type="ms:httpURI">
		<xs:annotation>
			<xs:documentation>Any url where the resource can be accessed from, either in a downloadable form or as a web executable command etc.</xs:documentation>
			<xs:appinfo>
				<relation>one-to-many</relation>
				<label>Download URL</label>
				<broader>dcat:downloadURL</broader>
				<recommended>true</recommended>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<!--
	<xs:element name="downloadURL" type="ms:httpURI">
		<xs:annotation>
			<xs:documentation>Any url where the resource can be downloaded from; please, use if the resource is downloadable and you have not uploaded the resource in the repository</xs:documentation>
			<xs:appinfo>
				<relation>one-to-many</relation>
				<label>Download URL</label>
				<sameAs>dcat:downloadURL</sameAs>
				<usage>Mandatory if distributionMedium=downloadable</usage>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="accessURL" type="ms:httpURI">
		<xs:annotation>
			<xs:documentation>A landing page, feed, SPARQL endpoint etc. that gives access to the resource or where the web service/workflow is executed; please use for resources that are "accessibleThroughInterface" or "webExecutable"</xs:documentation>
			<xs:appinfo>
				<relation>one-to-many</relation>
				<label>Access URL</label>
				<closeMatch>dcat:accessURL</closeMatch>
				<usage>Mandatory if distributionMedium=accessibleThroughInterface or webExecutable</usage>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
-->
	<xs:element name="copyrightStatement">
		<xs:annotation>
			<xs:documentation>A free text statement that may be included with the resource, usually containing the name(s) of copyright holders and licensing terms (intended for resources that already have such a statement); the element can be repeated for the different language versions using the "lang" attribute to specify the language.</xs:documentation>
			<xs:appinfo>
				<label>Copyright statement</label>
				<broadMatch>dct:rights</broadMatch>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:restriction base="ms:myString">
					<xs:maxLength value="500"/>
				</xs:restriction>
			</xs:simpleContent>
		</xs:complexType>
		<xs:unique name="uniqueLang200">
			<xs:selector xpath="."/>
			<xs:field xpath="@lang"/>
		</xs:unique>
	</xs:element>
	<xs:element name="attributionText">
		<xs:annotation>
			<xs:documentation>The text that must be quoted for attribution purposes when using a resource - for cases where a resource is provided with a restriction on attribution; you can use a standard text such as "Resource A by Resource Creator/Owner B used under licence C as accessed at D"</xs:documentation>
			<xs:appinfo>
				<relation>one-to-many</relation>
				<label>Attribution text</label>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:restriction base="ms:myString">
					<xs:maxLength value="1000"/>
				</xs:restriction>
			</xs:simpleContent>
		</xs:complexType>
		<xs:unique name="uniqueLang12">
			<xs:selector xpath="."/>
			<xs:field xpath="@lang"/>
		</xs:unique>
	</xs:element>
	<xs:element name="rightsHolder" type="ms:actorInfoType">
		<xs:annotation>
			<xs:documentation>Groups information on a person or an organization who holds the Copyright and SGDB rights that subsist in the resource. The rights holder could be different from the creator that may have assigned the rights to another person or organization (e.g. an author as a creator assigns her rights to the publisher who is the rights holder) and the distributor that holds a specific licence (i.e. a permission) to distribute the work.</xs:documentation>
			<xs:appinfo>
				<relation>many-to-many</relation>
				<label>Rights holder</label>
				<broadMatch>dct:rightsHolder</broadMatch>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="availabilityStartDate" type="xs:date">
		<xs:annotation>
			<xs:documentation>Specifies the start date of availability of a resource - only for cases where a resource is available for a restricted time period.</xs:documentation>
			<xs:appinfo>
				<relation>one-to-one</relation>
				<label>Availability start date</label>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="availabilityEndDate" type="xs:date">
		<xs:annotation>
			<xs:documentation>Specifies the end date of availability of a resource - only for cases where a resource is available for a restricted time period.</xs:documentation>
			<xs:appinfo>
				<relation>one-to-one</relation>
				<label>Availability end date</label>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="fee">
		<xs:annotation>
			<xs:documentation>Specifies the costs that are required to access the resource, a fragment of the resource or to use a component</xs:documentation>
			<xs:appinfo>
				<relation>one-to-one</relation>
				<label>Fee</label>
			</xs:appinfo>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="100"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="userType">
		<xs:annotation>
			<xs:documentation>The conditions imposed by the user type (for instance, use may be allowed only for academic users or the fee may vary depending on the user type)</xs:documentation>
			<xs:appinfo>
				<relation>one-to-many</relation>
				<label>User type</label>
			</xs:appinfo>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
				<xs:enumeration value="academic">
					<xs:annotation>
						<xs:documentation>For academic institutions</xs:documentation>
						<xs:appinfo>
							<label>Academic</label>
						</xs:appinfo>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="commercial">
					<xs:annotation>
						<xs:documentation>For commercial institutions</xs:documentation>
						<xs:appinfo>
							<label>Commercial</label>
						</xs:appinfo>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="membersOfGroup">
					<xs:annotation>
						<!-- CHECK would it be possible to add here an attribute for the group? -->
						<xs:documentation>For members of particular groups (e.g. META-SHARE, CLARIN, ELRA etc.)</xs:documentation>
						<xs:appinfo>
							<label>Members of group</label>
						</xs:appinfo>
					</xs:annotation>
				</xs:enumeration>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
</xs:schema>
