<?xml version="1.0" encoding="UTF-8"?>
<!-- edited by ILSP/Athena R.C. -->
<xs:schema xmlns:ms="http://www.meta-share.org/OMTD-SHARE_XMLSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.meta-share.org/OMTD-SHARE_XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0.1" xml:lang="en">
	<xs:include schemaLocation="./OMTD-SHARE-GlobalTypes.xsd"/>
	<xs:include schemaLocation="./OMTD-SHARE-ISOVocabularies.xsd"/>
	<xs:include schemaLocation="./OMTD-SHARE-Actors.xsd"/>
	<xs:include schemaLocation="./OMTD-SHARE-Projects.xsd"/>
	<xs:include schemaLocation="./OMTD-SHARE-Distributions.xsd"/>
	<!-- definition of complex types -->
	<xs:complexType name="mailingListInfoType">
		<xs:annotation>
			<xs:appinfo>
				<render-short>{mailingListName}</render-short>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="mailingListName" type="xs:string">
				<xs:annotation>
					<xs:documentation>The name of the mailing list</xs:documentation>
					<xs:appinfo>
						<label>Mailing list name</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="subscribe" type="ms:emailAddress" minOccurs="1">
				<xs:annotation>
					<xs:documentation>The email address to be used for subscribing to the mailing list</xs:documentation>
					<xs:appinfo>
						<label>Subscribe</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="unsubscribe" type="ms:emailAddress" minOccurs="1">
				<xs:annotation>
					<xs:documentation>The email address to be used for unsubscribing from the mailing list</xs:documentation>
					<xs:appinfo>
						<label>Unsubscribe</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="post" type="ms:emailAddress">
				<xs:annotation>
					<xs:documentation>The email address to be used for posting messages to the mailing list</xs:documentation>
					<xs:appinfo>
						<label>Post</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="archive" type="ms:httpURI">
				<xs:annotation>
					<xs:documentation>The url where the mailing list messages are archived</xs:documentation>
					<xs:appinfo>
						<label>Archive</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="otherArchives" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="otherArchive" type="ms:httpURI" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Alternative url's where the mailing list messages are archived (for mirrored mailing lists)</xs:documentation>
								<xs:appinfo>
									<label>Other archive</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="identificationInfoType">
		<xs:sequence>
			<xs:element name="resourceNames">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="ms:resourceName" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="descriptions" minOccurs="1" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="description" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Provides the description of the resource in prose; the element can be repeated for the different language versions using the "lang" attribute to specify the language.</xs:documentation>
								<xs:appinfo>
									<label>Description</label>
									<exactMatch>dct:description</exactMatch>
									<closeMatch>http://hdl.handle.net/11459/CCR_C-2520_9eeedfb4-47d3-ddee-cfcb-99ac634bf1db (ccr:descriptionContext)</closeMatch>
									<closeMatch>datacite:description descriptionType="Abstract"</closeMatch>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:restriction base="ms:myString">
										<xs:maxLength value="10000"/>
									</xs:restriction>
								</xs:simpleContent>
							</xs:complexType>
							<xs:unique name="uniqueLang3">
								<xs:selector xpath="."/>
								<xs:field xpath="@lang"/>
							</xs:unique>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="resourceShortNames" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="resourceShortName" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>The short form (abbreviation, acronym etc.) used to identify the resource; the element can be repeated for the different language versions using the "lang" attribute to specify the language.</xs:documentation>
								<xs:appinfo>
									<label>Resource short name</label>
									<closeMatch>dct:alternative</closeMatch>
									<exactMatch>http://hdl.handle.net/11459/CCR_C-2544_3626545e-a21d-058c-ebfd-241c0464e7e5 (ccr:resourceName)</exactMatch>
									<!-- in DataCite, the title has the attribute "titleType" with values "alternativeTitle", "translatedTitle" and "subtitle"; we can do something similar in the next version if desired -->
									<closeMatch>datacite:title titleType="alternativeTitle"</closeMatch>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:restriction base="ms:myString">
										<xs:maxLength value="500"/>
									</xs:restriction>
								</xs:simpleContent>
							</xs:complexType>
							<xs:unique name="uniqueLang">
								<xs:selector xpath="."/>
								<xs:field xpath="@lang"/>
							</xs:unique>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="identifiers" minOccurs="1" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="identifier" type="ms:resourceIdentifierType" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Reference to a PID, DOI or any kind of identifier used by the resource provider for the resource; the attribute documents the scheme it adheres to; if the scheme name is not included in the listed values, please use "other" and provide a link  to  information about the scheme</xs:documentation>
								<xs:appinfo>
									<label>Identifier</label>
									<exactMatch>dct:identifier</exactMatch>
									<!-- we allow for all possible identifiers and not only persistent ones as in CCR -->
									<closeMatch>http://hdl.handle.net/11459/CCR_C-2573_ae7c2548-8a86-ab6e-7099-e28b7697d1a2 (ccr:persistentIdentifier)</closeMatch>
									<!-- currently DataCite only supports DOI's -->
									<exactMatch>dataCite:identifier</exactMatch>
								</xs:appinfo>
							</xs:annotation>
							<!-- CHECK: I have not been able to add the restriction of length 100 -->
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="contactInfoType">
		<xs:sequence>
			<xs:choice>
				<xs:element name="contactEmail" type="ms:emailAddress">
					<xs:annotation>
						<xs:documentation>A general email address that can be used as contact point for a resource (e.g. resource@example.com); the recommended practice is, however, to indicate a person/group for contact and describe it with the relevant metadata elements</xs:documentation>
						<xs:appinfo>
							<label>Contact email</label>
							<!-- in DataCite, the contactPerson can also be used for contact points -->
							<closeMatch>datacite:contributor contributorType="ContactPerson"</closeMatch>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="landingPage" type="ms:httpURI">
					<xs:annotation>
						<xs:documentation>A URL used as the landing page of a resource providing general information; for instance, it may present a description of the resource, its creators and possibly include links to the URL where it can be accessed from</xs:documentation>
						<xs:appinfo>
							<label>Landing page</label>
							<exactMatch>dcat:landingPage</exactMatch>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="contactPersons" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="contactPerson" type="ms:relatedPersonType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Groups information on the person(s) that is/are responsible for providing further information regarding the resource</xs:documentation>
								<xs:appinfo>
									<label>Contact person</label>
									<exactMatch>dcat:contactPoint</exactMatch>
									<closeMatch>http://hdl.handle.net/11459/CCR_C-2454_e0e88868-2fc6-a87e-cd0d-d8f69c85b41d (ccr:contactFullName)</closeMatch>
									<relation>many-to-many</relation>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="contactGroups" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="contactGroup" type="ms:relatedGroupType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Groups information on the group(s) that is/are responsible for providing further information regarding the resource</xs:documentation>
								<xs:appinfo>
									<label>Contact group</label>
									<exactMatch>dcat:contactPoint</exactMatch>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<!-- ASK googlegroups?? -->
			<xs:element name="mailingLists" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="mailingListInfo" type="ms:mailingListInfoType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Groups information on the mailing list(s) that can be used for further information regarding the resource</xs:documentation>
								<xs:appinfo>
									<label>Mailing list</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="versionInfoType">
		<xs:annotation>
			<xs:appinfo>
				<render-short>{version} {revision} {lastDateUpdated}</render-short>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="version">
				<xs:annotation>
					<!-- ASK for semantic version of components and for types of versioning of publications (cf. CrossRef update) -->
					<xs:documentation>Any string, usually a number, that identifies the version of a resource; for components, the recommended practice is to follow the semantic versioning (http://semver.org/); it should not be confused with the relation that links together a specific resource with its various versions</xs:documentation>
					<xs:appinfo>
						<label>Version</label>
						<exactMatch>dataCite:version</exactMatch>
						<closeMatch>http://hdl.handle.net/11459/CCR_C-2547_7883d382-b3ce-8ab4-7052-0138525a8ba1 (ccr:version)</closeMatch>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="revision" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provides an account of the revisions in free text or a link to a document with revisions</xs:documentation>
					<xs:appinfo>
						<label>Revision</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="500"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="lastDateUpdated" type="ms:dateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date of the last update of the version of the resource</xs:documentation>
					<xs:appinfo>
						<label>Last date updated</label>
						<exactMatch>dct:modified</exactMatch>
						<exactMatch>http://hdl.handle.net/11459/CCR_C-2526_979ac535-eaa5-5e59-3cad-51c450234698 (ccr:lastUpdate)</exactMatch>
						<exactMatch>dataCite:date dateType="updated"</exactMatch>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="updateFrequency" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the frequency with which the resource is updated</xs:documentation>
					<xs:appinfo>
						<label>Update frequency</label>
						<closeMatch>dct:frequency</closeMatch>
						<closeMatch>dcat:accrualPeriodicity</closeMatch>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="validationInfoType">
		<xs:sequence>
			<xs:element name="validated" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Specifies the validation status of the resource</xs:documentation>
					<xs:appinfo>
						<label>Validated</label>
						<exactMatch>http://hdl.handle.net/11459/CCR_C-2584_36b81c9c-e23a-2ace-36cd-b0d6679e3197 (ccr:validation)</exactMatch>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="validationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the type of the validation that have been performed </xs:documentation>
					<xs:appinfo>
						<label>Validation type</label>
						<exactMatch>http://hdl.handle.net/11459/CCR_C-2587_026dcaa6-8ece-3364-8492-6479e70f66de (ccr:validationType)</exactMatch>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
						<xs:enumeration value="formal">
							<xs:annotation>
								<xs:appinfo>
									<label>Formal</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="content">
							<xs:annotation>
								<xs:appinfo>
									<label>Content</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="validationMode" type="ms:processMode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the validation methodology applied</xs:documentation>
					<xs:appinfo>
						<label>Validation mode</label>
						<exactMatch>http://hdl.handle.net/11459/CCR_C-2586_75098d25-a517-983f-817c-2b05c5ce361a (ccr:validationMode)</exactMatch>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="validationModeDetails" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Textual field for additional information on validation</xs:documentation>
					<xs:appinfo>
						<label>Validation mode details</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="500"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="validationExtent" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The resource coverage in terms of validated data</xs:documentation>
					<xs:appinfo>
						<label>Validation extent</label>
						<exactMatch>http://hdl.handle.net/11459/CCR_C-2585_0fcf8ea5-a273-d077-86e9-5e8f1c50fe72 (ccr:validationLevel)</exactMatch>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
						<xs:enumeration value="full"/>
						<xs:enumeration value="partial"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="validationExtentDetails" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provides information on size or other details of partially validated data; to be used if only part of the resource has been validated and as an alternative to SizeInfo if the validated part cannot be counted otherwise</xs:documentation>
					<xs:appinfo>
						<label>Validation extent details</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="500"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="sizePerValidation" type="ms:sizeInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the size of the validated part of a resource</xs:documentation>
					<xs:appinfo>
						<label>Size per validation</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="validationReports" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="hasValidationReport" type="ms:relatedDocumentInfoType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Link to a short account (prose description) of the validation details or the identifier of a document with detailed information on the validation process and results</xs:documentation>
								<xs:appinfo>
									<label>Has validation report</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="validationSwComponents" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="isValidatedBy" type="ms:relatedResourceType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Links a resource to the identifier of the software component that has been used for its validation</xs:documentation>
								<xs:appinfo>
									<label>Is validated by</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="validators" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="validator" type="ms:actorInfoType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Groups information on the person(s) or the organization(s) that validated the resource</xs:documentation>
								<xs:appinfo>
									<label>Validator</label>
									<broadMatch>dct:contributor</broadMatch>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="resourceCreationInfoType">
		<xs:annotation>
			<xs:appinfo>
				<render-short>{resourceCreator} {fundingProject}</render-short>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="resourceCreators" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="resourceCreator" type="ms:actorInfoType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Groups information on the person or the organization that has created the resource</xs:documentation>
								<xs:appinfo>
									<label>Resource creator</label>
									<exactMatch>dct:creator</exactMatch>
									<exactMatch>dataCite:creator</exactMatch>
									<closeMatch>http://hdl.handle.net/11459/CCR_C-2512_e8061b33-c5f3-7f6c-184d-a4311549ba92 (ccr:creatorFullName)</closeMatch>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="fundingProjects" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="fundingProject" type="ms:relatedProjectType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Groups information on the project that has funded the resource; choice between the ID of a project or a project title</xs:documentation>
								<xs:appinfo>
									<label>Funding project</label>
									<!-- I've put the link to dct:relation because of OpenAIRE; the syntax in OpenAIRE is info:eu-repo/grantAgreement/Funder/FundingProgram/ProjectID/
[Jurisdiction]/[ProjectName]/[ProjectAcronym] which can be split into the relevant elements -->
									<broadMatch>dct:relation</broadMatch>
									<narrowMatch>http://hdl.handle.net/11459/CCR_C-2522_3bdc6af1-bf1b-3f5d-2938-62d99a1980ab (ccr:funder)</narrowMatch>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="creationDate" type="ms:dateCombinationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date of the creation of hte resource, expressed as a range between starting and end date or exact date</xs:documentation>
					<xs:appinfo>
						<label>Creation date</label>
						<exactMatch>dct:created</exactMatch>
						<exactMatch>dataCite:date dateType="created"</exactMatch>
						<exactMatch>http://hdl.handle.net/11459/CCR_C-2510_2402e609-046a-dfbf-c2d7-5a2f1ae6dc86 (ccr:creationDate)</exactMatch>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- CHECK maybe rename as provenanceInfo and add here anything specfic to our project (i.e. coming from OpenAIRE, CORE, user query etc.) -->
	<xs:complexType name="creationInfoType">
		<xs:sequence>
			<xs:element name="originalSources" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="hasOriginalSource" type="ms:relatedResourceType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>The identifier of the original resources that were at the base of the creation process of the resource</xs:documentation>
								<xs:appinfo>
									<label>Has original source</label>
									<exactMatch>dct:source</exactMatch>
									<exactMatch>http://hdl.handle.net/11459/CCR_C-2534_04c8cf6a-4272-2213-4de7-52713b7c286f (ccr:originalSource)</exactMatch>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="creationMode" type="ms:processMode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies whether the resource is created automatically or in a manual or interactive mode </xs:documentation>
					<xs:appinfo>
