	<!-- definition of related entities -->
	<xs:complexType name="actorInfoType">
		<xs:annotation>
			<xs:documentation>Base type for actors/agents; choice between person and organization</xs:documentation>
			<xs:appinfo>
				<relation>one-to-one</relation>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice>
			<xs:element name="relatedPerson" type="ms:relatedPersonType">
				<xs:annotation>
					<xs:appinfo>
						<relation>one-to-one</relation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="relatedOrganization" type="ms:relatedOrganizationType">
				<xs:annotation>
					<xs:appinfo>
						<relation>one-to-one</relation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="relatedPersonType">
		<xs:annotation>
			<xs:documentation>Base type for related persons; choice between person names and person identifiers</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="personNames">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="personName" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>The name(s) of the person, preferrably in the format: surname, initials (first name) prefix</xs:documentation>
