	<!-- definition of metadata header -->
	<xs:complexType name="metadataHeaderInfoType">
		<xs:annotation>
			<xs:documentation>Base type for metadata header</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="metadataRecordIdentifier" type="ms:metadataIdentifierType">
				<xs:annotation>
					<xs:documentation>A persistent identifier for the metadata record itself</xs:documentation>
					<xs:appinfo>
						<label>Metadata record identifier</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="metadataCreationDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>The date of creation of this metadata description (automatically inserted by the software)</xs:documentation>
					<xs:appinfo>
						<label>Metadata creation date</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="metadataCreators" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="metadataCreator" type="ms:relatedPersonType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Groups information on the person that has created the metadata record (for records manually edited)</xs:documentation>
								<xs:appinfo>
									<relation>many-to-many</relation>
									<label>Metadata creator</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="sourceOfMetadataRecord" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Refers to the catalog or repository from which the metadata record has been originated (for harvested records)</xs:documentation>
					<xs:appinfo>
						<label>Source of metadata record</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="collectedFrom" type="ms:relatedRepositoryType">
							<xs:annotation>
								<xs:documentation>Refers to the entity (repository, aggregator etc.) from which the metadata record has been harvested</xs:documentation>
								<xs:appinfo>
									<label>Source repository</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="sourceMetadataLink" type="ms:httpURI">
							<xs:annotation>
								<xs:documentation>A link to the original metadata record, in cases of harvesting</xs:documentation>
								<xs:appinfo>
									<label>Original metadata record link</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="relatedMetadataScheme" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Refers to the metadata schema originally used for the description of the resource</xs:documentation>
								<xs:appinfo>
									<label>Original metadata scheme</label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="500"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="originalDataProviderInfo" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Refers to the original data provider, in case the metadata record carries information taken from previous repositories/journals/publishers (e.g. in case the record's last source is an aggregator)</xs:documentation>
								<xs:appinfo>
									<label>Original data provider</label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="originalDataProviderType" minOccurs="1">
										<xs:annotation>
											<xs:documentation>Refers to the type of the original data provider (repository/journal/publisher/aggregator etc.)</xs:documentation>
											<xs:appinfo>
												<label>Original data provider type</label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="repository"/>
												<xs:enumeration value="journal"/>
												<xs:enumeration value="publisher"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:choice>
										<xs:element name="originalDataProviderRepository" type="ms:relatedRepositoryType">
											<xs:annotation>
												<xs:documentation>Information about the repository that has originally provided the metadata record</xs:documentation>
												<xs:appinfo>
													<label>Original data provider (repository)</label>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="originalDataProviderJournal" type="ms:relatedJournalType">
											<xs:annotation>
												<xs:documentation>Information about the journal that has originally provided the metadata record</xs:documentation>
												<xs:appinfo>
													<label>Original data provider (journal)</label>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
										<xs:element name="originalDataProviderPublisher" type="ms:relatedOrganizationType">
											<xs:annotation>
												<xs:documentation>Information about the publisher (organization) that has originally provided the metadata record</xs:documentation>
												<xs:appinfo>
													<label>Original data provider (publisher)</label>
												</xs:appinfo>
											</xs:annotation>
										</xs:element>
									</xs:choice>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="userQuery" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The query text that has created the corpus of scholarly publications</xs:documentation>
					<xs:appinfo>
						<label>User query</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="metadataLanguages" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="metadataLanguage" type="ms:languageType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>The language in which the metadata description is written according to IETF BCP47 (ISO 639-1 or ISO 639-3 for languages not covered by the first standard)</xs:documentation>
								<xs:appinfo>
									<label>Metadata language</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="metadataLastDateUpdated" type="xs:date">
				<xs:annotation>
					<xs:documentation>The date of the last updating of the metadata record (automatically inserted by the system)</xs:documentation>
					<xs:appinfo>
						<label>Metadata last date updated</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="revision" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provides an account of the revisions in free text or a link to a document with revisions</xs:documentation>
					<xs:appinfo>
						<label>Revision</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="500"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- definition of re-usable elements -->
	<xs:element name="date" type="ms:dateType">
		<xs:annotation>
			<xs:documentation>Basic format for dates, with optional day and month and obligatory year</xs:documentation>
			<xs:appinfo>
				<label>Date</label>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="endDate" type="ms:dateType">
		<xs:annotation>
