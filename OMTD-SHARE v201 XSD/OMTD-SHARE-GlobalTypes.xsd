										<label>ISNI (International Standard Name Identifier)</label>
									</xs:appinfo>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="GRID">
								<xs:annotation>
									<xs:documentation>Value for organizationIdentifierSchemeName, referring to the GRID (Global Research Identifier Database, https://www.grid.ac/)</xs:documentation>
									<xs:appinfo>
										<label>GRID (Global Research Identifier Database)</label>
									</xs:appinfo>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="FundRef">
								<xs:annotation>
									<xs:documentation>Value for organizationIdentifierSchemeName, referring to the CrossRef Funder ID (https://www.crossref.org/services/funder-registry/)</xs:documentation>
									<xs:appinfo>
										<label>CrossRef Funder ID</label>
									</xs:appinfo>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="other">
								<xs:annotation>
									<xs:documentation>Value for organizationIdentifierSchemeName, to be used when the desired value is not included in the listed values</xs:documentation>
									<xs:appinfo>
										<label>other</label>
									</xs:appinfo>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute ref="ms:schemeURI" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
