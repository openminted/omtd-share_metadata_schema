								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
-->
			<xs:element name="metadataLastDateUpdated" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date of the last updating of the metadata record</xs:documentation>
					<xs:appinfo>
						<label>Metadata last date updated</label>
						<usage>Automatically inserted by the system</usage>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="revision" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provides an account of the revisions in free text or a link to a document with revisions</xs:documentation>
					<xs:appinfo>
						<label>Revision</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="500"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- definition of re-usable elements -->
	<xs:element name="date" type="ms:dateType">
		<xs:annotation>
			<xs:documentation>Basic format for dates, with optional day and month and obligatory year</xs:documentation>
			<xs:appinfo>
				<label>Date</label>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="endDate" type="ms:dateType">
		<xs:annotation>
			<xs:documentation>Basic format for end dates, with optional day and month and obligatory year</xs:documentation>
			<xs:appinfo>
				<label>End date</label>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="startDate" type="ms:dateType">
		<xs:annotation>
			<xs:documentation>Basic format for start dates, with optional day and month and obligatory year</xs:documentation>
			<xs:appinfo>
				<label>Start date</label>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="dateRange" type="ms:dateRangeType">
		<xs:annotation>
			<xs:documentation>Basic format for date ranges, allowing combinations of years and full dates</xs:documentation>
			<xs:appinfo>
				<label>Date range</label>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="subject">
		<xs:annotation>
			<xs:documentation>For resources that are using subject / topic classification</xs:documentation>
			<xs:appinfo>
				<label>Subject</label>
				<closeMatch>Topic</closeMatch>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="classificationSchemeName" type="ms:classificationSchemeName" use="required"/>
					<xs:attribute name="schemeURI" type="xs:anyURI" use="optional"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="metadataHeaderInfo" type="ms:metadataHeaderInfoType">
		<xs:annotation>
			<xs:documentation>Groups information on the metadata record itself</xs:documentation>
			<xs:appinfo>
				<label>Metadata header</label>
				<usage>Do not show in the editor forms</usage>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="resourceName">
		<xs:annotation>
			<xs:documentation>Specifies the name (full title) by which the resource is known</xs:documentation>
			<xs:appinfo>
				<xs:label>Resource name</xs:label>
				<xs:label-plural>Resource names</xs:label-plural>
				<!--
				<xs:userTip>For multilingual elements: you can repeat the element for the different language versions using the "lang" attribute to specify the language</xs:userTip>
-->
				<usage-ugc>To be created automatically with the following text: "OpenMinTeD subset of [CORE, OpenAIRE, ...] publications"</usage-ugc>
				<change>true</change>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:restriction base="ms:myString">
					<xs:maxLength value="500"/>
				</xs:restriction>
			</xs:simpleContent>
		</xs:complexType>
		<xs:unique name="uniqueLang2">
			<xs:selector xpath="."/>
			<xs:field xpath="@lang"/>
		</xs:unique>
	</xs:element>
	<xs:element name="organizationIdentifier" type="ms:organizationIdentifierType">
		<xs:annotation>
			<xs:documentation>Associates an organizationto an identifier, i.e. a string used to uniquely identify it</xs:documentation>
			<xs:appinfo>
				<label>Organization identifier</label>
				<label-plural>Organization identifiers</label-plural>
				<sameAs>dct:identifier</sameAs>
				<!-- we allow for all possible identifiers and not only persistent ones as in CCR -->
				<closeMatch>http://hdl.handle.net/11459/CCR_C-2573_ae7c2548-8a86-ab6e-7099-e28b7697d1a2 (ccr:persistentIdentifier)</closeMatch>
				<sameAs>dataCite:nameIdentifier</sameAs>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="personIdentifier" type="ms:personIdentifierType">
		<xs:annotation>
			<xs:documentation>Associates a person to an identifier (e.g. ORCID, LinkedIn etc.), i.e. a string used to uniquely identify it</xs:documentation>
			<xs:appinfo>
				<label>Person identifier</label>
				<label-plural>Person identifiers</label-plural>
				<sameAs>dct:identifier</sameAs>
				<!-- we allow for all possible identifiers and not only persistent ones as in CCR -->
				<closeMatch>http://hdl.handle.net/11459/CCR_C-2573_ae7c2548-8a86-ab6e-7099-e28b7697d1a2 (ccr:persistentIdentifier)</closeMatch>
				<sameAs>dataCite:nameIdentifier</sameAs>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="publicationIdentifier" type="ms:publicationIdentifierType">
		<xs:annotation>
			<xs:documentation>Reference to a DOI (recommended) or any kind of identifier used for the publication</xs:documentation>
			<xs:appinfo>
				<label>Publication identifier</label>
				<sameAs>dct:identifier</sameAs>
				<!-- we allow for all possible identifiers and not only persistent ones as in CCR -->
				<closeMatch>http://hdl.handle.net/11459/CCR_C-2573_ae7c2548-8a86-ab6e-7099-e28b7697d1a2 (ccr:persistentIdentifier)</closeMatch>
				<!-- currently DataCite only supports DOI's -->
				<narrowMatch>dataCite:identifier</narrowMatch>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="publisher" type="ms:organizationInfoType">
		<xs:annotation>
			<xs:documentation>Information about the publisher</xs:documentation>
			<xs:appinfo>
				<label>Publisher</label>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="projectIdentifier" type="ms:projectIdentifierType">
		<xs:annotation>
			<xs:documentation>A unique identifier for research projects</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="journalIdentifier" type="ms:journalIdentifierType">
		<xs:annotation>
			<xs:documentation>Reference to a DOI (recommended) or any kind of identifier used for the publication</xs:documentation>
			<xs:appinfo>
				<label>Journal Identifier</label>
				<sameAs>dct:identifier</sameAs>
				<!-- we allow for all possible identifiers and not only persistent ones as in CCR -->
				<closeMatch>http://hdl.handle.net/11459/CCR_C-2573_ae7c2548-8a86-ab6e-7099-e28b7697d1a2 (ccr:persistentIdentifier)</closeMatch>
				<!-- currently DataCite only supports DOI's -->
				<sameAs>dataCite:identifier</sameAs>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:attribute name="schemeURI" type="xs:anyURI">
		<xs:annotation>
			<xs:documentation>Specifies the URI of the scheme according to which the resource identifier is assigned by the authority that issues it</xs:documentation>
			<xs:appinfo>
				<label>URI for scheme</label>
				<label-plural>URI for schemes</label-plural>
				<userTip>If the scheme name is not included in the listed values for resource or other identifiers, please use "other" and use the "schemeURI" attribute to provide a link to information about the scheme</userTip>
				<conditions>Show in editor only if resourceIdentifierSchemeName=other or personIdentifierSchemeName=other</conditions>
			</xs:appinfo>
		</xs:annotation>
	</xs:attribute>
</xs:schema>
