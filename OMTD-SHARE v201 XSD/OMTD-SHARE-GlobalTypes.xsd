<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:ms="http://www.meta-share.org/OMTD-SHARE_XMLSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.meta-share.org/OMTD-SHARE_XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.0.1" xml:lang="en">
	<xs:include schemaLocation="./OMTD-SHARE-ISOVocabularies.xsd"/>
	<xs:include schemaLocation="./OMTD-SHARE-Actors.xsd"/>
	<xs:include schemaLocation="./OMTD-SHARE-Distributions.xsd"/>
	<!-- definition of global simple types -->
	<xs:simpleType name="httpURI">
		<xs:annotation>
			<xs:documentation>Base type to be used for url's, ftp's, secure url's</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:anyURI">
			<xs:pattern value="http://.*"/>
			<xs:pattern value="https://.*"/>
			<xs:pattern value="ftp://.*"/>
			<xs:pattern value="www.*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="emailAddress">
		<xs:annotation>
			<xs:documentation>Base type for validating email addresses</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="100"/>
			<xs:pattern value="[^@]+@[^\.]+\..+|"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- definition of reusable simpleTypes specific to resources -->
	<xs:simpleType name="processMode">
		<xs:annotation>
			<xs:documentation>Base type for all modes of processing (e.g. annotation, conversion, creation etc.)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
			<xs:enumeration value="manual">
				<xs:annotation>
					<xs:documentation>For processes performed only manually, without automatic means or aids</xs:documentation>
					<xs:appinfo>
						<label>Manual</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="automatic">
				<xs:annotation>
					<xs:documentation>For processes performed only with automatic means or aids</xs:documentation>
					<xs:appinfo>
						<label>Automatic</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="mixed">
				<xs:annotation>
					<xs:documentation>For processes performed in a mixed way, using manual and automatic means</xs:documentation>
					<xs:appinfo>
						<label>Mixed</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="interactive">
				<xs:annotation>
					<xs:documentation>For processes performed interactively</xs:documentation>
					<xs:appinfo>
						<label>Interactive</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!-- definition of simple types for attributes -->
	<xs:simpleType name="classificationSchemeName">
		<xs:annotation>
			<xs:documentation>Base type for listing recommended/popular classification schemes</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="DDC">
				<xs:annotation>
					<xs:documentation>Dewey Decimal Classification system; https://www.oclc.org/dewey.en.html</xs:documentation>
					<xs:appinfo>
						<label>Dewey Decimal Classification</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="UDC">
				<xs:annotation>
					<xs:documentation>Universal Decimal Classification system; for the Linked Data version, cf. http://udcdata.info/</xs:documentation>
					<xs:appinfo>
						<label>Universal Decimal Classification</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EuroVoc">
				<xs:annotation>
					<xs:documentation>EuroVoc, the EU's multilingual thesaurus; various downloadable versions at http://data.europa.eu/euodp/en/data/dataset/eurovoc</xs:documentation>
					<xs:appinfo>
						<label>EuroVoc</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LCSH">
				<xs:annotation>
					<xs:documentation>Library of Congress Subject Headings; cf. http://id.loc.gov/authorities/subjects.html</xs:documentation>
					<xs:appinfo>
						<label>Library of Congress Subject Headings</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DK-5">
				<xs:annotation>
					<xs:documentation>DK-5; Linked Data version at https://opensource.dbc.dk/linked-data/dk5-linked-data</xs:documentation>
					<xs:appinfo>
						<label>DK-5</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MeSH">
				<xs:annotation>
					<xs:documentation>Medical Subject Headlings; downloadable versions at https://www.nlm.nih.gov/mesh/download_mesh.html</xs:documentation>
					<xs:appinfo>
						<label>Medical Subject Headings</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ANC_classification">
				<xs:annotation>
					<xs:documentation>American National Corpus controlled vocabulary for text classification; cf. http://www.anc.org/</xs:documentation>
					<xs:appinfo>
						<label>American National Corpus classification system</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BNC_classification">
				<xs:annotation>
					<xs:documentation>British National Corpus controlled vocabulary for text classification; cf. http://www.natcorp.ox.ac.uk/docs/URG/ subsections on selection and descriptive features</xs:documentation>
					<xs:appinfo>
						<label>British National Corpus classification system</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NLK_classification"/>
			<xs:enumeration value="PAROLE_topicClassification">
				<xs:annotation>
					<xs:documentation>Parole topic classification system; used for the consistent classification of text corpora in the LE-PAROLE project</xs:documentation>
					<xs:appinfo>
						<label>PAROLE topic classification system</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PAROLE_genreClassification">
				<xs:annotation>
					<xs:documentation>Parole genre classification system; used for the consistent classification of text corpora in the LE-PAROLE project</xs:documentation>
					<xs:appinfo>
						<label>PAROLE genre classification system</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="other">
				<xs:annotation>
					<xs:documentation>Value for classificationSchemeName, to be used when the desired value is not included in the listed values</xs:documentation>
					<xs:appinfo>
						<label>Other</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
